    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-2017": "0.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "bulma": "^0.4.2",
    "chart.js": "^2.6.0",
    "css-loader": "^0.28.4",
    "dompurify": "^0.9.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "mongoose": "^4.10.5",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.2",
    "sass-loader": "^6.0.5",
    "socket.io": "^2.0.2",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
