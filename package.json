{
  "name": "stocks-charts",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "description": "Chart the stock market",
  "main": "start.js",
  "scripts": {
    "start": "npm run build && node ./start.js",
    "watch": "nodemon ./start.js --ignore public/",
